{
  "manifest_version": 3,
  "name": "Whisk Image Generator Bot",
  "version": "1.0.0",
  "description": "Automate image generation on Google Whisk by blending images and prompts",
  "permissions": [
    "tabs",
    "storage",
    "downloads",
    "scripting",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://labs.google/*",
    "https://labs.google/fx/tools/whisk/project",
    "http://localhost:5000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://labs.google/*",
        "https://labs.google/fx/tools/whisk/project"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}